{
  "name": "palenight-veryperi",
  "publisher": "milanteo",
  "author": "Matteo Alborghetti <milanteo@live.it>",
  "displayName": "Palenight Veryperi Theme",
  "version": "0.0.1",
  "description": "An elegant and juicy material-inspired theme for Visual Studio Code.",
  "galleryBanner": {
    "color": "#31364A",
    "theme": "dark"
  },
  "icon": "icon.png",
  "keywords": [
    "material theme",
    "palenight",
    "material",
    "purple",
    "dark"
  ],
  "repository": {
    "url": "https://github.com/whizkydee/vscode-palenight-theme",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/whizkydee/vscode-palenight-theme/issues"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.12.0"
  },
  "scripts": {
    "start": "nodemon --exec vsce package",
    "build": "nodemon --watch src src"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Palenight Very Peri Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/palenight.json"
      },
      {
        "label": "Palenight Very Peri Italic",
        "uiTheme": "vs-dark",
        "path": "./themes/palenight-italic.json"
      },
      {
        "label": "Palenight Very Peri Operator",
        "uiTheme": "vs-dark",
        "path": "./themes/palenight-operator.json"
      },
      {
        "label": "Palenight Very Peri (Mild Contrast)",
        "uiTheme": "vs-dark",
        "path": "./themes/palenight-mild-contrast.json"
      }
    ]
  },
  "devDependencies": {
    "nodemon": "^1.12.1",
    "vsce": "^1.88.0"
  },
  "__metadata": {
    "publisherDisplayName": "Olaolu Olawuyi"
